config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "HexCell",
"language": "GDScript",
"path": "res://combat/hexgrid/HexCell.gd"
}, {
"base": "Reference",
"class": "HexCoords",
"language": "GDScript",
"path": "res://combat/hexgrid/HexCoords.gd"
} ]
_global_script_class_icons={
"HexCell": "",
"HexCoords": ""
}

[application]

config/name="hommlite"
run/main_scene="res://combat/CombatScene.tscn"
